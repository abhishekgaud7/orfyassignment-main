{
    "version": 2,
    "buildCommand": "cd Frontend && npm install && npm run build",
    "outputDirectory": "Frontend/dist",
    "functions": {
        "Backend/server.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/Backend/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}