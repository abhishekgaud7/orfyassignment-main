{
    "version": 2,
    "buildCommand": "cd Frontend && npm install && npm run build",
    "outputDirectory": "Frontend/dist",
    "functions": {
        "Backend/server.js": {
            "runtime": "vercel-node@latest"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/Backend/server.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}